{
	"name": "gulp-template",
	"version": "1.0.0",
	"description": "",
	"main": "gulpfile.js",
	"type": "module",
	"scripts": {
		"dev": "gulp",
		"build": "gulp build --build",
		"ghPages": "(git branch -D gh-pages || echo \"Delete local branch failed. It is possible that local branch exists\") && git checkout --orphan gh-pages && gulp build --build && git rm -f --cached -r . && find dist/ | xargs git add && git commit -m \"Init gh-pages\" && git push --set-upstream origin gh-pages --force && git checkout -f main && git pull",
		"zip": "gulp deployZIP --build",
		"deploy": "gulp deployFTP --build",
		"fonts": "gulp fonts",
		"updateFiles": "gulp updateFiles"
	},
	"author": "",
	"license": "ISC",
